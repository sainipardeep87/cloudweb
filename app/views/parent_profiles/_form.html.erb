  <div class="field">
    <%= f.label :device_type_id %><br />
    <%= f.select :status, [['Andriod', 0], ['Iphone', 1],['blackberry',2]] %>
  </div>
  <div class="field">
    <%= f.label :imei %><br />
    <%= f.text_field :imei %>
  </div>
  <div class="field">
    <%= f.label :token_id %><br />
    <%= f.text_field :token_id %>
  </div>

    <div class="field">
      <%= f.label :status %><br />
      <%= f.select :status, [['Active', true], ['Inactive', false]] %>
    </div>
    <div class="field">
      <%= f.label :name %><br />
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :is_machine_owner %><br />
      <%= f.select :is_machine_owner, [['Yes', true], ['No', false]] %>
    </div>

    <div class="field">
      <%= f.label :machine_serial_id %><br />
      <%= f.number_field :machine_serial_id %>
    </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
