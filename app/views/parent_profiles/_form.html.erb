<%= form_for(@parent_profile) do |f| %>
  <% if @parent_profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@parent_profile.errors.count, "error") %> prohibited this parent_profile from being saved:</h2>

      <ul>
      <% @parent_profile.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
     <div class="label_div"><%= f.label :name %></div>
     <div class="value_div"><%= f.text_field :name,:size => 20 %></div>
  </div>

  <div class="field">
    <div class="label_div"><%= f.label :machine_serial_id %></div>
    <div class="value_div"><%= f.number_field :serialid,:size => 20 %></div>
  </div>

    <div class="field">
    <div class="label_div"> <%= f.label :devicetypeid %></div>
    <div class="value_div"> <%= f.select :devicetypeid, [['Andriod', 0], ['Iphone', 1],['blackberry',2]] %></div>
  </div>

  <div class="field">
    <div class="label_div"><%= f.label :status %></div>
    <div class="value_div"><%= f.select :status, [['Active', true], ['Inactive', false]] %></div>
  </div>
    <!--<div class="field">-->
    <!--<div class="label_div"><%= f.label :token_id %></div>-->
      <!--<div class="value_div"><%= f.text_field :tokenid %></div>-->
  <!--</div>-->
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
