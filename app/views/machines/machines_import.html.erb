<h3> Machines > Import</h3>
<div class="csv_format">
  <%= link_to "Download CSV format" ,{:controller=>"/machines",:action=>"machine_csv_download",:format => 'xls'}%>
  <!--<a  style="color: deepskyblue" href="#">Download CSV format</a>-->
</div>

<%= form_for :machine_upload , :url=>{:action=>'import_machine_csv'} do |f| %>
    <div class="field">
      <p>
        <%=f.label :select_file%>
        <%=f.file_field :file%>
      </p>
    </div>
    <div>
      <%=f.submit :uplaod%>
    </div>

<%end%>